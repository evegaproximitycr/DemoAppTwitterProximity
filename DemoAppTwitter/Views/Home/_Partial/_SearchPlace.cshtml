@model IList<string>
    
@if (Model.Count > 0)
{

    <div class="panel-body">
        <table class="table table-bordered table-striped table-condensed">
            <thead>
                <tr>
                    <th>Top 5 tweets</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var tweet in Model)
                {
                    <tr>
                        <td>@tweet</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>

}


